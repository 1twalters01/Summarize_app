{
  "plugins": [],
  "recurseDepth": 10,
  "compilerOptions": {
    "strict": true,
    "checkJs": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "allowImportingTsExtensions": true
  },
  "source": {
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnkownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
